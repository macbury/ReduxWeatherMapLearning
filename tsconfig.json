{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react"
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "files": [
    "./typings/index.d.ts",
    "./src/components/app.tsx",
    "./src/index.tsx"
  ],
  "filesGlob": [
    "./typings/index.d.ts",
    "./src/**/*.ts",
    "./src/**/*.tsx"
  ],
  "atom": {
    "rewriteTsconfig": true
  },
  "compileOnSave": false
}
